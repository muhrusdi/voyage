{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import",
      {
        "libraryName": "antd",
        "style": false
      }
    ],
    "styled-jsx/babel",
    ["emotion", { "inline": true , "importedNames": { "css": "emotion" }}]
  ]
}